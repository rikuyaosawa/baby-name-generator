<%- include("../partials/header.ejs") %>
<%- include("../partials/edit-main-header.ejs") %>

<body class="edit-main">

	<div class="db-block">
		<h1 class="edit-main-header"><%= gender %> Names</h1>
		<h3>Total number of names: <%= numOfRows %></h3>
		<table>
			<tr>
				<th>ID</th>
				<th>NAME</th>
				<th>MEANING</th>
				<th>NICKNAME</th>
				<th>ORIGIN</th>
				<th>VARIATION</th>
			</tr>
			<% data.forEach(name => { %>
				<tr>
					<td><%- name.id %></td>
					<td><%- name.name %></td>
					<td><%- name.meaning %></td>
					<td><%- name.nickname %></td>
					<td><%- name.origin %></td>
					<td><%- name.variation %></td>
				</tr>
			<% }); %>
		</table>
	</form>
</div>

</body>

	<%- include("../partials/edit-footer.ejs") %>
